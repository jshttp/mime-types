{
  "name": "@whykhamist/mime-types",
  "description": "The ultimate javascript content-type utility.",
  "private": true,
  "version": "1.0.0",
  "contributors": [
    "Jhyden Jhoe Marx Ellana <https://github.com/whykhamist>",
    "Douglas Christopher Wilson <doug@somethingdoug.com>",
    "Jeremiah Senkpiel <fishrock123@rocketmail.com> (https://searchbeam.jit.su)",
    "Jonathan Ong <me@jongleberry.com> (http://jongleberry.com)"
  ],
  "type": "module",
  "license": "MIT",
  "keywords": [
    "mime",
    "types"
  ],
  "repository": "whykhamist/mime-types",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index",
  "types": "./dist/types/index.d.ts",
  "files": [
    "HISTORY.md",
    "LICENSE",
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/index",
      "require": "./dist/index.umd.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./src/*": "./src/*"
  },
  "scripts": {
    "build": "vue-tsc -b && npm run build:lib && npm run build:dts",
    "build:lib": "vite build",
    "build:dts": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
    "lint": "eslint src/**/*.ts",
    "test": "npm run build && mocha --reporter spec test/*.spec.{ts,cjs}",
    "test:no-build": "mocha --reporter spec test/*.spec.{ts,cjs}"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/chai": "^4.3.16",
    "@types/eslint__js": "^8.42.3",
    "@types/mime-db": "^1.43.5",
    "@types/mocha": "^10.0.7",
    "chai": "^5.1.1",
    "eslint": "^8.57.0",
    "mocha": "^10.7.0",
    "prettier": "^3.3.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0",
    "vite": "^5.3.5",
    "vue-tsc": "^2.0.29"
  },
  "dependencies": {
    "mime-db": "^1.53.0"
  }
}
